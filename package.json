{
  "name": "cache-sticky",
  "version": "1.0.0",
  "description": "A minimal frontend-only web application to visualize browser Cache API behavior through sticky notes",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
